{
  "name": "css-uiapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "format:prettier:check": "./node_modules/.bin/prettier --config ./.prettierrc --list-different \"src/**/*.{ts,js,json,css}\" -l",
    "format:prettier:fix": "./node_modules/.bin/prettier --config ./.prettierrc --list-different \"src/**/*.{ts,js,json,css}\" --write",
    "format:html:check": "node bin/glob-ls.js 'src/**/*.html'  | xargs  ./bin/html-beautify-check.sh",
    "format:html:fix": "node bin/glob-ls.js 'src/**/*.html'  | xargs ./node_modules/.bin/html-beautify -r",
    "format:check": "npm run format:prettier:check && npm run format:html:check",
    "format:fix": "npm run format:prettier:fix && npm run format:html:fix",
    "lint:fix": "ng lint --fix",
    "format:lint:fix": "npm run format:fix && npm run lint:fix",
    "compodoc:gen": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "compodoc:serve": "./node_modules/.bin/compodoc -s src/tsconfig.app.json",
    "compodoc": "npm run compodoc:gen && npm run compodoc:serve",
    "build:alllang:prod": "ng build --prod && ng build --configuration=production-fr",
    "build:alllang:qa": "ng build --configuration=qa-en && ng build --configuration=qa-fr",
    "build:alllang:dev": "ng build --configuration=dev-en && ng build --configuration=dev-fr",
    "extract-i18n": "ng xi18n css-uiapp --i18n-format xlf --output-path i18n --i18n-locale en && ng run css-uiapp:xliffmerge",
    "start-dev-en": "ng serve --configuration=dev-en",
    "start-dev-fr": "ng serve --configuration=dev-fr --port 4300"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.0",
    "@angular/cdk": "~8.2.2",
    "@angular/common": "^8.1.0",
    "@angular/compiler": "^8.1.0",
    "@angular/core": "^8.1.0",
    "@angular/forms": "^8.1.0",
    "@angular/material": "^8.2.2",
    "@angular/platform-browser": "^8.1.0",
    "@angular/platform-browser-dynamic": "^8.1.0",
    "@angular/router": "^8.1.0",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@types/intro.js": "^2.4.4",
    "@types/socket.io-client": "^1.4.32",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.9",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "intro.js": "^2.9.3",
    "jquery": "^3.3.1",
    "ngx-custom-validators": "^8.0.0",
    "ngx-logger": "^3.4.5",
    "popper.js": "^1.15.0",
    "rxjs": "^6.5.2",
    "socket.io-client": "^2.3.0",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "^8.1.0",
    "@angular/compiler-cli": "^8.1.0",
    "@angular/language-service": "^8.1.0",
    "@compodoc/compodoc": "^1.1.10",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^8.10.51",
    "codelyzer": "^5.0.1",
    "husky": "^2.7.0",
    "jasmine-core": "^2.99.1",
    "jasmine-spec-reporter": "^4.2.1",
    "js-beautify": "^1.10.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "^1.18.2",
    "protractor": "^5.4.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~3.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:check && npm run lint"
    }
  }
}
